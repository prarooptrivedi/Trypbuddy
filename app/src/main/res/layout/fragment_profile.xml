<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/chooseFileRect"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/rv_first"
        android:layout_width="fill_parent"
        android:background="@color/travelcolor"
        android:layout_height="50dip">
        <com.example.praroop.trypbuddycom.Customfont.MyTextView_Roboto_Regular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Profile"
            android:textColor="@color/white"
            android:textSize="18dip"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dip"
            android:layout_marginLeft="15dip"/>
    </RelativeLayout>
    <ScrollView
        android:layout_marginTop="10dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">


    <android.support.v7.widget.CardView
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        app:cardCornerRadius="1dip"
        android:layout_marginRight="10dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        ></android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        app:cardCornerRadius="1dip"
        android:layout_marginRight="10dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Notification Sound"
                android:textSize="16dip"
                android:layout_gravity="center"
                android:layout_marginLeft="15dip"
                android:textColor="@color/black"/>
            <Switch
                android:layout_width="match_parent"
                android:layout_marginRight="15dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center|right"/>
        </LinearLayout>
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="1dip"
    android:layout_marginRight="10dip"
    android:layout_marginLeft="10dip"
    android:layout_marginTop="10dip"
    >
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dip">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="My Bookings"
                    android:textSize="16dip"
                    android:layout_gravity="center"
                    android:layout_marginLeft="15dip"
                    android:textColor="@color/black"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:background="#dedede"></LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dip">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="My Wishist"
                    android:textSize="16dip"
                    android:layout_gravity="center"
                    android:layout_marginLeft="15dip"
                    android:textColor="@color/black"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:background="#dedede"></LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dip">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Offers"
                    android:textSize="16dip"
                    android:layout_gravity="center"
                    android:layout_marginLeft="15dip"
                    android:textColor="@color/black"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0.5dip"
                android:background="#dedede"></LinearLayout>
        </LinearLayout>

</android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="1dip"
            android:layout_marginRight="10dip"
            android:layout_marginLeft="10dip"
            android:layout_marginTop="10dip"
            >
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="How it works?"
                        android:textSize="16dip"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dip"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="#dedede"></LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Blog"
                        android:textSize="16dip"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dip"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="#dedede"></LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rate the app"
                        android:textSize="16dip"
                        android:layout_gravity="center"
                        android:layout_marginLeft="15dip"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.5dip"
                    android:background="#dedede"></LinearLayout>
            </LinearLayout>

        </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="1dip"
                android:layout_marginRight="10dip"
                android:layout_marginLeft="10dip"
                android:layout_marginTop="10dip"
                >
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Need Help ? Call Us!"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:id="@+id/layout_callback"
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Request Call Back"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="1dip"
                android:layout_marginRight="10dip"
                android:layout_marginLeft="10dip"
                android:layout_marginTop="10dip"
                >

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/linear_aboutus"
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="About Us"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                    <LinearLayout
                        android:id="@+id/linear_terms"
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Terms and Conditions"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                    <LinearLayout
                        android:id="@+id/linear_privacy"
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Privacy Policy"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                    <LinearLayout
                        android:id="@+id/linear_faq"
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Faq"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dip"
                        android:background="#dedede"></LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Contact US"
                            android:textSize="16dip"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dip"
                            android:textColor="@color/black"/>

                    </LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_marginTop="20dip"
                android:layout_marginLeft="20dip"
                android:layout_marginRight="20dip"
                android:layout_marginBottom="60dip"
                android:background="@drawable/rounded"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_height="50dip">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="SIGN OUT"
                    android:textSize="15dip"
                    android:layout_gravity="center"
                    android:textColor="@color/create_color"/>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>